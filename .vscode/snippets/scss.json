{
  "scroll": {
    "scope": "css, scss",
    "prefix": "c-scroll",
    "body": [
      ".scroll {",
      "  @include flex-center;",
      "  width: 65px;",
      "  height: 65px;",
      "  position: fixed;",
      "  right: 50px;",
      "  bottom: 50px;",
      "  font-size: 40px;",
      "  color: $1#ffffff;",
      "  border-radius: 50%;",
      "  background: $2#808080;",
      "  z-index: 100;",
      "  cursor: pointer;",
      "  opacity: 1;",
      "  transition: color 0.5s ease-in-out;",
      "",
      "  &:hover {",
      "    color: $0#000000;",
      "  }",
      "",
      "  &--hidden {",
      "    opacity: 0;",
      "  }",
      "}"
    ],
    "description": "SCSS snippet for scroll"
  },

  "background-color: $1;": {
    "scope": "css, scss",
    "prefix": "bg",
    "body": ["background-color: $1;"],
    "description": "CSS, SCSS snippet for background"
  },

  "background-position: center;": {
